package pkTest;

import java.io.*;
import javax.swing.*;

public class Test {

	public static void main(String[] args) {
		
		JFrame frame = new JFrame();
		
		JButton button = new JButton("Test bouton");
		
		button.setBounds(150, 200, 220, 50);
		
		frame.add(button);
		
		frame.setSize(1920, 1080);
		frame.setLayout(null);
		frame.setVisible(true);

	}

}
