package pkTest;

import pkClasse.*;
import java.util.*;

public class TestTache {
	
	final public static int MAXELEMENT = 5;
	
	private static Tache donnees[] = 
		{
				new Tache("Projet Nouveauté Prog", new GregorianCalendar(2023, 3, 28), new GregorianCalendar(2023, 5, 31), 10, 50, "Bientot fini?"),
				new Tache("Projet Nouveauté Sys", new GregorianCalendar(2023, 2, 28), new GregorianCalendar(2023, 4, 12), 5, 20, "Pas commencer"),
				new Tache("Synthèse web", new GregorianCalendar(2023, 4, 15), new GregorianCalendar(2023, 6, 30), 15, 20, "Pas reçu"),
				new Tache("Quiz BD", new GregorianCalendar(2023, 5, 15), new GregorianCalendar(2023, 5, 15), 1, 20, "Vraiment pas long"),
				new Tache("Acheter du lait", new GregorianCalendar(2023, 5, 15), new GregorianCalendar(2023, 5, 15), 2, 7, "C'est trop bon"),
		};

	public static void main(String[] args) {
		
		ArrayList bdTaches;
		
		bdTaches = new ArrayList(Arrays.asList(donnees)); 
		System.out.println(bdTaches.toString());

	}

}
